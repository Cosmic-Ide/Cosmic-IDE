<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<com.google.android.material.appbar.AppBarLayout
		android:id="@+id/appbar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:theme="@style/Theme.JavaIDE.AppBarOverlay">
		<androidx.appcompat.widget.Toolbar
			android:id="@+id/toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="?attr/colorPrimary"
			app:popupTheme="@style/Theme.JavaIDE.PopupOverlay" />
	</com.google.android.material.appbar.AppBarLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_behavior="@string/appbar_scrolling_view_behavior">
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/versionTxt"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="4dp"
			android:text="@string/java_version"
			android:textSize="16sp"
			android:textStyle="bold"
			android:textColor="#000000" />
		<HorizontalScrollView
			android:id="@+id/hscroll3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<RadioGroup
				android:id="@+id/javaGroup"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="1.3"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="1.4"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="5.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="6.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="7.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="8.0"
					android:textSize="12sp"
					android:textColor="#000000" />
					<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="9.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java10"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="10.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java11"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="11.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="12.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java13"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="13.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java14"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="14.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java15"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="15.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java16"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="16.0"
					android:textSize="12sp"
					android:textColor="#000000" />
				<com.google.android.material.radiobutton.MaterialRadioButton
					android:id="@+id/java17"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="17.0"
					android:textSize="12sp"
					android:textColor="#000000" />
			</RadioGroup>
		</HorizontalScrollView>
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/classpathTxt"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="4dp"
			android:text="Classpath"
			android:textSize="16sp"
			android:textStyle="bold"
			android:textColor="#000000" />
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/textinputlayout1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			app:placeholderText="separated by ':'"
			style="@theme/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
			<androidx.appcompat.widget.AppCompatEditText
				android:id="@+id/classpath"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:textSize="12sp"
				android:textColor="#000000" />
		</com.google.android.material.textfield.TextInputLayout>
	</LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>